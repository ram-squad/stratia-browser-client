		"@typescript-eslint/parser": "6.12.0",
		"eslint": "8.54.0",
		"eslint-plugin-import": "2.29.0",
		"eslint-plugin-svelte": "2.35.1",
		"prettier": "3.1.0",
		"prettier-plugin-svelte": "3.1.2",
		"release-it": "16.3.0",
		"sass": "1.69.5",
		"svelte": "4.2.7",
		"svelte-check": "3.6.2",
		"svelte-eslint-parser": "0.33.1",
		"svelte-preprocess": "5.1.1",
		"testcontainers": "10.3.2",
		"ts-essentials": "9.4.1",
		"typescript": "5.3.2",
